      Result.BtnSaveSetting.OnMouseLeave := Result.BtnSaveMouseLeave;
      Result.BtnSave.OnClick := Result.BtnSaveClick;
      Result.BtnSave.OnMouseMove := Result.BtnSaveMouseMove;
      Result.BtnSave.OnMouseLeave := Result.BtnSaveMouseLeave;
      Result.BtnLoadSetting.OnClick := Result.BtnLoadSettingClick;
      Result.BtnLoadSetting.OnMouseMove := Result.BtnSaveMouseMove;
      Result.BtnLoadSetting.OnMouseLeave := Result.BtnSaveMouseLeave;
