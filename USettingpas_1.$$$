      Result.CBLanguage.OnChange := Result.CBLanguageChange;
      Result.CBSkin.OnChange := Result.CBSkinChange;
    Result.EBAdzanFile.Color := TColor(clWindow);
      Result.BtnPlay.OnClick := Result.BtnPlayClick;
      Result.BtnPlay.OnMouseMove := Result.BtnSaveMouseMove;
      Result.BtnPlay.OnMouseLeave := Result.BtnSaveMouseLeave;
      Result.BtnPause.OnClick := Result.BtnPauseClick;
      Result.BtnPause.OnMouseMove := Result.BtnSaveMouseMove;
      Result.BtnPause.OnMouseLeave := Result.BtnSaveMouseLeave;
      Result.BtnStop.OnClick := Result.BtnStopClick;
      Result.BtnStop.OnMouseMove := Result.BtnSaveMouseMove;
      Result.BtnStop.OnMouseLeave := Result.BtnSaveMouseLeave;
      Result.BtnBrowse.OnClick := Result.BtnBrowseClick;
      Result.BtnBrowse.OnMouseMove := Result.BtnSaveMouseMove;
